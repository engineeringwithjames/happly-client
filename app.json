{
  "expo": {
    "name": "Happly",
    "slug": "happly-client",
    "version": "4.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios"
    ],
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.engineeringwithjames.happlyclient",
      "buildNumber": "4.0.0",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "mailto"
        ]
      }
    },
    "owner": "engineeringwithjames",
    "android": {
      "package": "com.engineeringwithjames.happlyclient"
    },
    "extra": {
      "eas": {
        "projectId": "97884f8f-6e23-49d1-88d5-80c95a31d81f"
      }
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
